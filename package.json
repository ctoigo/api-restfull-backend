{
  "name": "api-restfull-backend",
  "version": "1.0.0",
  "description": "API RestFULL Backend para consultas",
  "private": true,
  "author": {
    "name": "Chrystian Toigo"
  },
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "start": "node dist/main/server.js",
    "dev": "ts-node-dev --respawn -r tsconfig-paths/register --no-notify src/main/dev.ts",
    "build": "rimraf dist && babel src --extensions .ts --out-dir dist --copy-files --no-copy-ignored",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "yarn test -- --watch -c jest-unit-config.js",
    "test:integration": "yarn test -- --watch -c jest-integration-config.js",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test -- --coverage",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-winston": "^4.2.0",
    "module-alias": "^2.2.3",
    "response-time": "^2.3.2",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.10.0",
    "youch": "^3.2.3",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.11",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.36",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.4",
    "@types/response-time": "^2.3.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/winston": "^2.4.4",
    "@types/yup": "^0.29.14",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^38.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "*"
  },
  "engines": {
    "node": "18.x"
  },
  "jest": {
    "verbose": true
  },
  "_moduleAliases": {
    "@/": "dist"
  }
}
